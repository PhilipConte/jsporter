{
  "name": "jsporter",
  "productName": "jsporter",
  "version": "1.0.1",
  "description": "An attempt at emulating the 'contacts' portion of Sidekick98",
  "repository": {
    "type": "git",
    "url": "git://github.com/PhilipConte/jsporter"
  },
  "author": {
    "name": "Philip Conte",
    "email": "philipmconte@gmail.com",
    "url": "https://github.com/PhilipConte"
  },
  "homepage": "http://www.github.com/PhilipConte/jsporter",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "keywords": ["electron", "react", "material-ui", "sequelize", "sqlite", "javascript"],
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "jsporter"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "PhilipConte",
        "name": "jsporter"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "jsporter"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^1.2.3",
    "@material-ui/icons": "^1.1.0",
    "autobind-decorator": "^2.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^2.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "sequelize": "^4.37.10",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.3",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2"
  }
}
